`env

# GENERAL
NEXT_PUBLIC_APP_URL=
NEXT_PUBLIC_APP_NAME=
NODE_ENV=

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Flutterwave Configuration
NEXT_PUBLIC_FLUTTERWAVE_PUBLIC_KEY=
FLUTTERWAVE_SECRET_KEY=
FLUTTERWAVE_ENCRYPTION_KEY=
FLUTTERWAVE_WEBHOOK_SECRET=

# Cloudinary Configuration
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=marketplace_unsigned
CLOUDINARY_API_SECRET=

# NIN Verification (Youverify)
YOUVERIFY_API_KEY=your_youverify_api_key_here
YOUVERIFY_BASE_URL=https://api.youverify.co/v2
YOUVERIFY_WEBHOOK_SECRET=your_webhook_secret_here

# Rate Limiting
UPSTASH_REDIS_REST_URL=your_redis_url
UPSTASH_REDIS_REST_TOKEN=your_redis_token

# JWT secret (for additional auth operations)
JWT_SECRET=your-very-long-random-secret-key-here-minimum-32-chars

# Rate limiting
RATE_LIMIT_WINDOW=60000  # 1 minute in ms
RATE_LIMIT_MAX=100       # 100 requests per window

# FEATURE FLAGS
NEXT_PUBLIC_ENABLE_LOCATION_DETECTION=true
NEXT_PUBLIC_ENABLE_UNIVERSITY_FILTER=true
NEXT_PUBLIC_ENABLE_ESCROW=true

# DEVELOPMENT ONLY
# ============================================================================
# Set to 'true' to use test mode for payments
NEXT_PUBLIC_USE_TEST_PAYMENTS=true

# Set to 'true' to skip email verification in development
SKIP_EMAIL_VERIFICATION=false

# Marketplace Configuration
NEXT_PUBLIC_MARKETPLACE_ENABLED=true
MARKETPLACE_MIN_PRODUCT_PRICE=100
MARKETPLACE_MAX_PRODUCT_PRICE=10000000
MARKETPLACE_MAX_IMAGES=8

NEXT_PUBLIC_APP_NAME=F9
NEXT_PUBLIC_APP_URL=https://f9.ng

# note that this is free tier
UPSTASH_REDIS_REST_URL=https://worthy-quagga-6583.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_token_here

ENCRYPTION_KEY=generate_32_byte_hex_key_here