{
  "name": "f9-marketplace",
  "version": "1.0.0",
  "description": "Nigeria's Premier Student Freelance Marketplace",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:smoke": "npm run test -- --testPathPattern=smoke",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset-db.js",
    "deploy:staging": "bash scripts/deploy-staging.sh",
    "deploy:production": "bash scripts/deploy-production.sh",
    "postbuild": "next-sitemap",
    "analyze": "ANALYZE=true next build",
    "bundle-analyzer": "ANALYZE=true next build",
    "pre-commit": "npm run lint:fix && npm run format && npm run type-check",
    "pre-push": "npm run test && npm run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:all": "rm -rf .next out node_modules",
    "security:audit": "npm audit --production",
    "security:fix": "npm audit fix",
    "update:deps": "npx npm-check-updates -u && npm install",
    "update:deps:minor": "npx npm-check-updates -u --target minor && npm install"
  },
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.22.0",
    "@hookform/resolvers": "^5.2.2",
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.7",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.25",
    "isomorphic-dompurify": "^2.34.0",
    "lucide-react": "^0.556.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^9.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.68.0",
    "redis": "^5.10.0",
    "server-only": "^0.0.1",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.1.6",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.18",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
